// set of statement written for doing a specific task is called functions.
function fun(a:number,b:number):number{ //aur yha hr parameter ka type batana pdega
    return a+b;
}
let result=fun(20,30);
  console.log(result);




// agr kuch return nhi krta toh hum function ka return type void de denge
  function fun():void{
    console.log("called");
}

fun();






class person{
   names:string | null =null;
   age:number | null=null;

   fun():void{
    console.log("fun");
   }
}

let p =new person();


p.fun();
console.log(p.names='tanya');
console.log(p.age=21);
